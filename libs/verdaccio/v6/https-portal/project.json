{
  "root": "libs/verdaccio/v6/https-portal",
  "sourceRoot": "libs/verdaccio/v6/https-portal/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/verdaccio/v6/https-portal",
        "main": "libs/verdaccio/v6/https-portal/src/index.ts",
        "tsConfig": "libs/verdaccio/v6/https-portal/tsconfig.lib.json",
        "assets": ["libs/verdaccio/v6/https-portal/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/verdaccio/v6/https-portal/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/verdaccio/v6/https-portal"],
      "options": {
        "jestConfig": "libs/verdaccio/v6/https-portal/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:config", "scope:verdaccio"]
}
