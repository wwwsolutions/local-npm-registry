{
  "root": "libs/verdaccio/v5/reverse-proxy",
  "sourceRoot": "libs/verdaccio/v5/reverse-proxy/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/verdaccio/v5/reverse-proxy",
        "main": "libs/verdaccio/v5/reverse-proxy/src/index.ts",
        "tsConfig": "libs/verdaccio/v5/reverse-proxy/tsconfig.lib.json",
        "assets": ["libs/verdaccio/v5/reverse-proxy/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/verdaccio/v5/reverse-proxy/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/verdaccio/v5/reverse-proxy"],
      "options": {
        "jestConfig": "libs/verdaccio/v5/reverse-proxy/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:config", "scope:verdaccio"]
}
